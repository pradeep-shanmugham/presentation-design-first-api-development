@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}


<div class="table100 ver1 m-b-110">
    <div class="table100-head">
        <table>
            <thead>
                <tr class="row100 head">
                    <th class="cell100 column1">VIN</th>
                    <th class="cell100 column2">Make</th>
                    <th class="cell100 column3">Model</th>
                    <th class="cell100 column4">Regular Price</th>

                    <th class="cell100 column5">In Stock</th>
                    <th class="cell100 column6">On Sales</th>
                    <th class="cell100 column7">Sales Discount %</th>

                    <th class="cell100 column8">Savings</th>
                </tr>
            </thead>
        </table>
    </div>
    <div class="table100-body js-pscroll ps ps--active-y">
        <table>
            <tbody>
                @foreach (var item in @Model.Cars)
                {
                    <tr class="row100 body">
                        <td class="cell100 column1">@item.vinNo</td>
                        <td class="cell100 column2">@item.type.make</td>
                        <td class="cell100 column3">@item.type.model</td>
                        <td class="cell100 column4">@item.regularPrice</td>
                        <td class="cell100 column5">@item.isInStock</td>
                        <td class="cell100 column6">@item.isOnSale</td>
                        <td class="cell100 column7">@item.saleDiscountPercentage</td>
                        <td class="cell100 column8">@item.savings</td>
                    </tr>
                }
            </tbody>
        </table>
        <div class="ps__rail-x" style="left: 0px; bottom: 0px;"><div class="ps__thumb-x" tabindex="0" style="left: 0px; width: 0px;"></div></div><div class="ps__rail-y" style="top: 0px; height: 585px; right: 5px;"><div class="ps__thumb-y" tabindex="0" style="top: 0px; height: 293px;"></div></div>
    </div>
</div>
